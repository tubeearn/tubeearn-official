<!DOCTYPE html><html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>TubeEarn Owner Dashboard</title>
  <style>
    * { margin: 0; padding: 0; box-sizing: border-box; font-family: sans-serif; }
    body { background: #f9f9f9; color: #333; }/* Top Bar */
.topbar {
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #111;
  color: #fff;
  padding: 15px 20px;
}
.topbar .welcome { font-size: 18px; }
.topbar .profile {
  width: 40px; height: 40px;
  background: #fff; border-radius: 50%;
  cursor: pointer;
}

/* Menu Bar */
.menubar {
  display: flex;
  overflow-x: auto;
  background: #222;
  padding: 10px;
}
.menubar div {
  min-width: 150px;
  padding: 10px;
  margin-right: 10px;
  background: #333;
  color: #fff;
  border-radius: 8px;
  text-align: center;
  cursor: pointer;
  flex-shrink: 0;
}
.menubar div:hover {
  background: #444;
}

.content {
  padding: 20px;
}
.section {
  display: none;
  padding: 20px;
  background: #fff;
  border-radius: 8px;
  margin-bottom: 10px;
}
.active { display: block; }

  </style>
</head>
<body>
  <div class="topbar">
    <div class="welcome">Welcome to Owner Mr. Sanjeev Kumar</div>
    <div class="profile" title="Profile"></div>
  </div>  <div class="menubar">
    <div onclick="showSection('default')">Default Home</div>
    <div onclick="showSection('revenue')">Revenue</div>
    <div onclick="showSection('userdata')">User Data</div>
    <div onclick="showSection('wallet')">Wallet</div>
    <div onclick="showSection('adsense')">AdSense</div>
    <div onclick="showSection('setting')">Setting</div>
    <div onclick="showSection('monetization')">Monetization</div>
    <div onclick="showSection('aisanjeev')">AiSanjeev Command</div>
    <div onclick="showSection('security')">Security</div>
    <div onclick="showSection('updates')">Updates Switch</div>
    <div onclick="showSection('sync')">Supabase to Firebase (Both)</div>
    <div onclick="showSection('maintenance')">Maintenance</div>
    <div onclick="showSection('logout')">Logout</div>
  </div>  <div class="content">
    <div id="default" class="section active">This is Default Home (Live Summary will show here)</div>
    <div id="revenue" class="section">Revenue Dashboard</div>
    <div id="userdata" class="section">User Data List</div>
    <div id="wallet" class="section">Wallet Transactions</div>
    <div id="adsense" class="section">AdSense Integration</div>
    <div id="setting" class="section">Settings Panel</div>
    <div id="monetization" class="section">Monetization Control</div>
    <div id="aisanjeev" class="section">AI Sanjeev Command Module</div>
    <div id="security" class="section">Security Logs & Controls</div>
    <div id="updates" class="section">Update Switch Control</div>
    <div id="sync" class="section">Syncing Supabase â†” Firebase</div>
    <div id="maintenance" class="section">Maintenance Control Panel</div>
    <div id="logout" class="section">Logging Out...</div>
  </div>  <script>
    function showSection(id) {
      document.querySelectorAll('.section').forEach(sec => sec.classList.remove('active'));
      document.getElementById(id).classList.add('active');
    }
  </script></body>
</html>
